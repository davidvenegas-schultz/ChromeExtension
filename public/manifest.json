{
  "manifest_version": 3,
  "name": "FetchIT",
  "description": "The FetchIT Chrome Extension",
  "version": "0.1",
  "action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "permissions": [
    "scripting",
    "notifications",
    "webNavigation",
    "activeTab",
    "tabs",
    "<all_urls>"
  ],
  "host_permissions": [
    "https://*/",
    "http://*/"
  ],
  "background": {
    "scripts": ["EzVet.js"]
  }
}